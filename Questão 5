#include <stdio.h>
#include <string.h>
int main (){
  
    char nome[15],sobrenome[50],cpf[11];

    printf("Insira seu nome: ");
    scanf("%s",&nome);
    printf("Insira seu sobrenome: ");
    scanf("%s",&sobrenome);
    printf("Insira seu cpf: ");
    scanf("%s",&cpf);
    strncat(nome,sobrenome,1);
    strncat(nome,cpf,3);
    printf("%s", nome);
   
}
